  <system.web>
    <!-- Defined in KILOBYTES (binary representation) -->
    <httpRuntime maxRequestLength="1048576" />
  </system.web>

  <system.webServer>
    <security>
      <requestFiltering>
        <!-- This will handle requests up to 1GB -->
        <!-- Defined in BYTES (binary representation) -->
        <requestLimits maxAllowedContentLength="1073741824" />
      </requestFiltering>
    </security>
  </system.webServer>
</configuration>